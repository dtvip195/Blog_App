 <div class="container-fluid">
<!-- DataTables Example -->
  <div class="card mb-3">
    <div class="card-header">
      <div class="row">
        <div>
        <h3><i class="fas fa-table"></i>
         <%= t(".list_topic") %>
        </h3>
        </div>
        <div style="margin-left: 1000px;" id="">
          <%= link_to new_admin_post_path, class:"btn btn-info btn-lg" do %>
            <i class='fas fa-plus'></i> Add Post
          <% end %>
        </div>
      </div>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
          <thead>
            <tr>
              <th style="width: 200px;">Post Name</th>
              <th style="width: 600px;">Content</th>
              <th>Image</th>
              <th>Status</th>
              <th style="width: 120px;">Topic Name</th>
              <th style="width: 150px;">Category Name</th>
              <th>Hashtags</th>
              <th></th>
              <th></th>
            </tr>
          </thead>
          <tbody id="post">
            <%= render "post" %>
          </tbody>
        </table>
      </div>
    </div>
    <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div>
  </div>
</div>
